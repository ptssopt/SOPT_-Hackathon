<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_child_01"
    tools:context=".Activity.ChildRequestActivity">
    <LinearLayout
        android:layout_width="693dp"
        android:layout_height="262dp"
        android:layout_marginTop="49dp"
        android:layout_marginLeft="47dp"
        android:layout_marginBottom="49dp"
        >
        <ImageView
            android:layout_width="262dp"
            android:layout_height="262dp"
            android:src="@drawable/lego_princess"
            />
        <LinearLayout
            android:layout_width="431dp"
            android:layout_height="262dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="431dp"
                android:layout_height="31dp"
                android:layout_marginTop="23dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="67dp"
                    android:textSize="15sp"
                    android:textColor="#fcd108"
                    android:letterSpacing="-0.04"
                    android:text="미션명"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:textSize="21sp"
                    android:textColor="#f9f9f9"
                    android:letterSpacing="-0.03"
                    android:text="일주일간 일기 꼬박꼬박 쓰기!"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="431dp"
                android:layout_height="31dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:textSize="15sp"
                    android:textColor="#fcd108"
                    android:letterSpacing="-0.04"
                    android:text="선물"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:textSize="21sp"
                    android:textColor="#f9f9f9"
                    android:letterSpacing="-0.03"
                    android:text="레고 사주기"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="415dp"
                android:layout_marginLeft="18dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star1"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="111dp"
                    android:layout_marginRight="8dp"
                    android:src="@drawable/img_star_unactive"
                    />
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star2"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginRight="8dp"
                    android:src="@drawable/img_star_unactive"
                    />
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star3"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginRight="8dp"
                    android:src="@drawable/img_star_unactive"
                    />
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star4"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginRight="7dp"
                    android:src="@drawable/img_star_unactive"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="415dp"
                android:layout_height="131dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star5"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="152dp"
                    android:src="@drawable/img_star_unactive"
                    />
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star6"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="17dp"
                    android:src="@drawable/img_star_unactive"
                    />
                <ImageView
                    android:id="@+id/iv_mission_state_missionimage_star7"
                    android:layout_width="64dp"
                    android:layout_height="56dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="16dp"
                    android:src="@drawable/img_star_unactive"
                    />
            </LinearLayout>


        </LinearLayout>
