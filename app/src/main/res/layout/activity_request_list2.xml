<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#f9f9f9"
    tools:context=".Activity.RequestListActivity2">

    <TextView
        android:id="@+id/text_title"
        android:textAlignment="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@id/rv_request_list"
        android:layout_alignTop="@id/rv_request_list"
        android:layout_alignEnd="@id/rv_request_list"
        android:layout_alignBottom="@id/rv_request_list"
        android:layout_marginStart="281dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="271dp"
        android:layout_marginBottom="268dp"
        android:letterSpacing="-0.61"
        android:text="자녀의 승인 요청 내역"
        android:textColor="#7d6baf"
        android:textSize="21sp" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_request_list"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:background="@android:color/transparent" />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="78sp"
        android:id="@+id/view"
        android:layout_alignParentBottom="true"
        android:background="#fcd108">

    </FrameLayout>




</RelativeLayout>